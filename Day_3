
import java.util.*;
import java.lang.*;
import java.io.*;

class third_code
{
	public static void main (String[] args) throws java.lang.Exception
	{
Scanner scanner = new Scanner(System.in);
        int T = scanner.nextInt();
        while (T-- > 0) {
            int x = scanner.nextInt();
            int y = scanner.nextInt();
            System.out.println(Math.max(x - y, 0));	}
}
    
}
